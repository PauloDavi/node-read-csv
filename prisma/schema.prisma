// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Products {
  id          String @id @default(uuid())
  code_bar    String
  description String
  price       Float
  quantity    Int

  @@map("products")
}
